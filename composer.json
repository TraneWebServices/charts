{
    "name": "t/charts",
    "license": "MIT",
    "homepage": "https://tws.custura.de/",
    "description": "Laravel Livewire Charts.",
    "keywords": [
        "tws",
        "laravel",
        "livewire",
        "charts"
    ],
    "authors": [
        {
            "name": "Custură Laurențiu",
            "email": "mail@custura.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "livewire/livewire": "^3.0",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "T\\Charts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "T\\Charts\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "T\\Charts\\ChartsServiceProvider"
            ],
            "aliases": {
                "Charts": "T\\Charts\\Facades\\Charts"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
